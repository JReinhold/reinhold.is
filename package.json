{
  "name": "reinhold.is",
  "version": "2.0.0",
  "description": "Jeppe Reinhold's personal website",
  "author": "jreinhold <jeppe@reinhold.is>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "turbo run preview",
    "format": "prettier --plugin-search-dir=. --write .",
    "format:check": "prettier --plugin-search-dir=. --list-different .",
    "sync": "svelte-kit sync",
    "lint": "eslint .",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "svelte-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "test:watch": "playwright test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "turbo run validate-all",
    "prepare": "svelte-kit sync"
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "open-props": "^1.4.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.25.0",
    "@sveltejs/adapter-cloudflare": "7.0.3",
    "@sveltejs/kit": "^2.21.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.13",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "postcss": "^8.4.16",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "suncalc": "^1.9.0",
    "svelte": "^5.33.10",
    "svelte-check": "^4.2.1",
    "tslib": "^2.3.1",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "type": "module"
}
