{
  "name": "reinhold.is",
  "version": "2.0.0",
  "description": "Jeppe Reinhold's personal website",
  "author": "jreinhold <jeppe@reinhold.is>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@8.6.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "turbo run preview",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "sync": "svelte-kit sync",
    "lint": "eslint .",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "svelte-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "test:watch": "playwright test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "turbo run validate-all",
    "prepare": "svelte-kit sync"
  },
  "dependencies": {
    "@giscus/svelte": "^3.0.0",
    "@octokit/graphql": "^8.1.1",
    "open-props": "^1.7.5",
    "postcss-preset-env": "^9.6.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-reading-time": "^2.0.1",
    "remark-rehype": "^11.1.0",
    "remark-shiki-twoslash": "^3.1.3",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5",
    "vfile-matter": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.3",
    "@shikijs/rehype": "^1.11.1",
    "@shikijs/twoslash": "^1.11.1",
    "@sveltejs/adapter-cloudflare": "4.7.0",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.4",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.39",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "suncalc": "^1.9.0",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "svelte-preprocess": "^6.0.2",
    "tslib": "^2.6.3",
    "turbo": "^2.0.9",
    "typescript": "^5.5.4",
    "vite": "^5.3.4"
  },
  "type": "module",
  "pnpm": {
    "patchedDependencies": {
      "remark-shiki-twoslash@3.1.3": "patches/remark-shiki-twoslash@3.1.3.patch",
      "reading-time@1.5.0": "patches/reading-time@1.5.0.patch"
    }
  }
}
