{
  "name": "reinhold.is",
  "version": "2.0.0",
  "private": true,
  "description": "Jeppe Reinhold's personal website",
  "license": "MIT",
  "author": "jreinhold <jeppe@reinhold.is>",
  "type": "module",
  "scripts": {
    "build": "vite build && rm .svelte-kit/cloudflare/_worker.js.map",
    "dev": "vite dev",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "lint": "eslint .",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "start": "turbo run preview",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "svelte-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "sync": "svelte-kit sync",
    "test": "playwright test",
    "test:watch": "playwright test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "turbo run validate-all"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@giscus/svelte": "^3.0.0",
    "@iconify/json": "^2.2.230",
    "@playwright/test": "^1.45.3",
    "@shikijs/rehype": "^1.11.1",
    "@shikijs/transformers": "^1.11.1",
    "@shikijs/twoslash": "^1.11.1",
    "@sveltejs/adapter-cloudflare": "4.7.0",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.4",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.4",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.8.0",
    "mdsvex": "^0.11.2",
    "mdsvex-reading-time": "^1.0.4",
    "open-props": "^1.7.5",
    "postcss": "^8.4.39",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "remark-gfm": "^4.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "suncalc": "^1.9.0",
    "svelte": "5.0.0-next.198",
    "svelte-check": "^3.8.4",
    "svelte-preprocess": "^6.0.2",
    "tslib": "^2.6.3",
    "turbo": "^2.0.9",
    "typescript": "^5.5.4",
    "unplugin-icons": "^0.19.0",
    "vite": "^5.3.4",
    "zod": "^3.23.8"
  },
  "packageManager": "pnpm@9.6.0"
}
