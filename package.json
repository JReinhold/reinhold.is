{
  "name": "reinhold.is",
  "version": "2.0.0",
  "description": "Jeppe Reinhold's personal website",
  "author": "jreinhold <jeppe@reinhold.is>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@8.6.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "turbo run preview",
    "format": "prettier --plugin-search-dir=. --write .",
    "format:check": "prettier --plugin-search-dir=. --list-different .",
    "sync": "svelte-kit sync",
    "lint": "eslint .",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "svelte-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "test:watch": "playwright test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "turbo run validate-all",
    "prepare": "svelte-kit sync"
  },
  "dependencies": {
    "@giscus/svelte": "^2.2.8",
    "@octokit/graphql": "^5.0.6",
    "open-props": "^1.5.8",
    "postcss-preset-env": "^8.4.2",
    "rehype-sanitize": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-reading-time": "^2.0.1",
    "remark-rehype": "^10.1.0",
    "remark-shiki-twoslash": "^3.1.3",
    "remark-stringify": "^10.0.3",
    "unified": "^10.1.2",
    "vfile-matter": "^4.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.0",
    "@sveltejs/adapter-cloudflare": "2.2.4",
    "@sveltejs/kit": "^1.20.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "suncalc": "^1.9.0",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.5.3",
    "turbo": "^1.10.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "type": "module",
  "pnpm": {
    "patchedDependencies": {
      "remark-shiki-twoslash@3.1.3": "patches/remark-shiki-twoslash@3.1.3.patch",
      "reading-time@1.5.0": "patches/reading-time@1.5.0.patch"
    }
  }
}
