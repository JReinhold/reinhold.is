{
  "name": "reinhold.is",
  "version": "2.0.0",
  "description": "Jeppe Reinhold's personal website",
  "author": "jreinhold <jeppe@reinhold.is>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@7.2.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "start": "turbo run preview",
    "format": "prettier --plugin-search-dir=. --write .",
    "check:format": "prettier --plugin-search-dir=. --list-different .",
    "check:ts": "tsc --noEmit",
    "check:astro": "astro check",
    "check:svelte": "svelte-check",
    "check": "turbo run check-all"
  },
  "devDependencies": {
    "@astrojs/deno": "^0.1.5",
    "@astrojs/svelte": "^0.1.4",
    "@iconify-icons/fa6-brands": "^1.2.3",
    "@tsconfig/svelte": "^3.0.0",
    "astro": "1.0.0-beta.50",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.12",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "0.1.0-next.5",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "turbo": "^1.2.16"
  },
  "dependencies": {
    "astro-icon": "^0.7.1",
    "open-props": "^1.3.16"
  }
}
