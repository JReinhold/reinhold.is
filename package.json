{
  "name": "reinhold.is",
  "version": "2.0.0",
  "description": "Jeppe Reinhold's personal website",
  "author": "jreinhold <jeppe@reinhold.is>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@7.2.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "start": "turbo run preview",
    "format": "prettier --plugin-search-dir=. --write .",
    "check:format": "prettier --plugin-search-dir=. --list-different .",
    "check:lint": "eslint .",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "check:svelte:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "check:ts": "tsc --noEmit",
    "check:ts:watch": "tsc --noEmit --watch",
    "check:test": "playwright test",
    "check": "turbo run check-all"
  },
  "dependencies": {
    "open-props": "^1.3.16"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@sveltejs/adapter-cloudflare": "1.0.0-next.23",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.4",
    "cssnano": "^5.1.12",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.5",
    "tslib": "^2.3.1",
    "turbo": "^1.2.16",
    "typescript": "^4.7.2"
  },
  "type": "module"
}
