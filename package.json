{
  "name": "reinhold.is",
  "version": "2.0.0",
  "private": true,
  "description": "Jeppe Reinhold's personal website",
  "license": "MIT",
  "author": "jreinhold <jeppe@reinhold.is>",
  "type": "module",
  "scripts": {
    "build": "vite build && echo \"_worker.js: $(du -k .svelte-kit/cloudflare/_worker.js | cut -f1) KB\"",
    "dev": "vite dev",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "lint": "eslint .",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "start": "turbo run preview",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "svelte-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "sync": "svelte-kit sync",
    "test": "playwright test",
    "test:watch": "playwright test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "turbo run validate-all"
  },
  "dependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.30.1",
    "@giscus/svelte": "^3.1.0",
    "@iconify/json": "^2.2.349",
    "@playwright/test": "^1.53.0",
    "@shikijs/rehype": "^1.11.1",
    "@shikijs/transformers": "^1.11.1",
    "@sveltejs/adapter-cloudflare": "7.0.4",
    "@sveltejs/kit": "^2.21.5",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.7",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.10.1",
    "globals": "^16.3.0",
    "mdsvex": "^0.11.2",
    "mdsvex-reading-time": "^1.0.6",
    "open-props": "^1.7.15",
    "postcss": "^8.5.5",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-env": "^10.2.3",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "remark-gfm": "^4.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "shiki": "^3.7.0",
    "suncalc": "^1.9.0",
    "svelte": "5.34.3",
    "svelte-check": "^4.2.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.6.3",
    "turbo": "^2.5.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.35.1",
    "unplugin-icons": "^0.19.0",
    "vite": "^6.3.5",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "7.0.3",
    "@sveltejs/kit": "^2.21.1",
    "@types/node": "24.0.1",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.13",
    "postcss": "^8.4.16",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "suncalc": "^1.9.0",
    "svelte": "^5.33.10",
    "svelte-check": "^4.2.1",
    "tslib": "^2.3.1",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "packageManager": "pnpm@10.12.1"
}
