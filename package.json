{
  "name": "reinhold.is",
  "version": "2.0.0",
  "description": "Jeppe Reinhold's personal website",
  "author": "jreinhold <jeppe@reinhold.is>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@7.2.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "turbo run preview",
    "format": "prettier --plugin-search-dir=. --write .",
    "format:check": "prettier --plugin-search-dir=. --list-different .",
    "lint": "eslint .",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "svelte-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "test:watch": "playwright test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "turbo run validate-all"
  },
  "dependencies": {
    "open-props": "^1.4.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.25.0",
    "@sveltejs/adapter-cloudflare": "1.0.0-next.31",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.13",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.16",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "suncalc": "^1.9.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.5",
    "tslib": "^2.3.1",
    "turbo": "^1.4.3",
    "typescript": "^4.7.2",
    "vite": "^3.0.8"
  },
  "type": "module"
}
